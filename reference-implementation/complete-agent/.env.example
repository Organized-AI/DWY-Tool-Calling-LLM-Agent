# DWY Tool Calling LLM Agent Configuration
# Copy this to .env and fill in your actual values

# Server Configuration
PORT=3000
NODE_ENV=development

# Workshop 1: Project Planning (DeepInvent.ai)
DEEPINVENT_API_KEY=your-deepinvent-api-key-here
DEEPINVENT_BASE_URL=https://api.deepinvent.ai

# Workshop 2: Knowledge Graph (Penumbra.ai + Neo4j)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password
PENUMBRA_API_KEY=your-penumbra-api-key-here

# Workshop 3: Marketing AI Services (Sindarin.tech)
SINDARIN_API_KEY=your-sindarin-api-key-here
SINDARIN_VOICE_ID=default

# Workshop 4: AI Video Generation (Texel.ai)
TEXEL_API_KEY=your-texel-api-key-here
TEXEL_TEMPLATE_ID=default

# Workshop 5: Tool Calling & MCP Integration (Toolhouse.ai)
TOOLHOUSE_API_KEY=your-toolhouse-api-key-here
TOOLHOUSE_PROVIDER=default

# Workshop 6: Local AI Deployment (WebAI.com + Ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:latest
WEBAI_CONFIG_URL=https://webai.com/api/config

# Security
JWT_SECRET=your-super-secret-jwt-key-change-this
API_KEY=your-api-key-for-authentication

# Development
DEBUG=true
LOG_LEVEL=info