{
  "name": "workshop2-knowledge-graph-implementation",
  "version": "1.0.0",
  "description": "Knowledge graph system for AI memory and data relationships using Neo4j and Penumbra.ai",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "demo": "DEMO_MODE=true node index.js",
    "test": "jest",
    "setup": "node setup.js",
    "neo4j:start": "docker-compose up -d",
    "neo4j:stop": "docker-compose down",
    "neo4j:reset": "docker-compose down -v && docker-compose up -d",
    "load-schema": "node scripts/load-schema.js",
    "load-demo-data": "node scripts/load-demo-data.js"
  },
  "keywords": [
    "ai",
    "knowledge-graph",
    "neo4j",
    "penumbra",
    "memory-system",
    "graph-database"
  ],
  "author": "Organized AI // Vibe Coders Hackathon",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "neo4j-driver": "^5.15.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "chalk": "^4.1.2",
    "ws": "^8.14.2",
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@types/node": "^20.8.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
